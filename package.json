{
  "name": "cfcontactform1",
  "version": "1.0.0",
  "description": "Cloudflare Worker Contact Form with Turnstile, Mailgun, and Discord integration",
  "main": "worker.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:preview": "wrangler deploy --env preview",
    "tail": "wrangler tail",
    "setup": "./setup.sh",
    "kv:list": "wrangler kv:namespace list",
    "secrets:list": "wrangler secret list",
    "test:turnstile": "wrangler kv:key list --binding=turnstile_kv",
    "test:discord": "wrangler kv:key list --binding=Discord_Urls"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "contact-form",
    "turnstile",
    "mailgun",
    "discord"
  ],
  "author": "TAS Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/taslabs-net/cfcontactform1.git"
  },
  "devDependencies": {
    "wrangler": "^4.15.0"
  }
}